@using MvcKutuphaneUdemy.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Üye: @ViewBag.uyegcmss</h2>
<br />
<table class="table table-light">

    <tr>
        <th>Kitap ADI</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>

    @foreach (var gecmis in Model)
    {
        <tr>
            <td>@gecmis.TBLKITAP.AD</td>
            <td>@gecmis.PERSONEL</td>
            <td>@Convert.ToDateTime(gecmis.ALISTARIH).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(gecmis.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>