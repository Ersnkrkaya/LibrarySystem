@using MvcKutuphaneUdemy.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}

<h2>Giden Mesajlar Sayfası</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Alıcı</th>
            <th>Konu</th>
            <th>Mesaj</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mesaj in Model)
        {
            <tr>
                <td>@mesaj.ALICI</td>
                <td>@mesaj.KONU</td>
                <td>@mesaj.ICERIK</td>
                <td>@mesaj.TARIH</td>
            </tr>
        }

</tbody>
</table>

