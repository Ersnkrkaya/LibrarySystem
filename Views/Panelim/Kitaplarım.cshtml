@using MvcKutuphaneUdemy.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "Kitaplarım";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}
<br />
<h2>Kitap Geçmişim</h2>
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Kitap Adı</th>
            <th>Alış Tarihi</th>
            <th>Üye İade Tarihi</th>
        </tr>
    </thead>
    <tbody>
         @foreach (var k in Model)
        {
            <tr>
                <td>@k.TBLKITAP.AD</td>
                <td>@k.ALISTARIH</td>
                <td>@k.IADETARIH</td>
            </tr>
        }
    </tbody>
</table>